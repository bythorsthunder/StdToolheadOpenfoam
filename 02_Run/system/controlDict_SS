FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime; //firstTime; startTime; latestTime;

startTime       0.0;

stopAt          endTime; // endTime; nextWrite; writeNow; noWriteNow

endTime         0.06; // was 0.06

deltaT          2.5e-6;

writeControl    adjustableRunTime;

writeInterval   5e-5;

purgeWrite      3;

writeFormat     ascii;

writePrecision   10;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  off;

maxCo           0.5;
maxAlphaCo      1;

maxDeltaT       0.002;

functions
{
